{
	"version": "0.1.0",
	"configurations": [
        {
            "name": "Python",
            "type": "python",
            "pythonPath":"/usr/bin/python3.5", 
            "request": "launch",
            "stopOnEntry": false,
            "externalConsole":false,
            "program": "${file}",
            "debugOptions": [
                "WaitOnAbnormalExit",
                "WaitOnNormalExit",
                "RedirectOutput"
            ],
            "env": {"name":"value"},
            "args": ["-f", "Madeline*" ]
        }
	]
}